<endpoint xmlns="http://ws.apache.org/ns/synapse" name="EP2" >
	<http uri-template="http://localhost:8080/orchestration-1.0-SNAPSHOT/hello/name/{uri.var.no}/{uri.var.name}" method="get">
		<suspendOnFailure>
			<progressionFactor>1.0</progressionFactor>
		</suspendOnFailure>
		<markForSuspension>
			<retriesBeforeSuspension>0</retriesBeforeSuspension>
			<retryDelay>0</retryDelay>
		</markForSuspension>
	</http>
</endpoint>